{
  "name": "tagged",
  "version": "0.1.0",
  "description": "A Tag bookmark for web browsers",
  "scripts": {
    "dev": "webpack",
    "build-only": "export NODE_ENV=production && webpack",
    "build": "npm run image && export NODE_ENV=production && webpack",
    "image": "mkdir -p build && svg-m2png -vfo build -t \"#f2f2f2\" res/icons.svg"
  },
  "author": "Gustavo Vargas <xgvargas@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "coffee-loader": "^0.7.3",
    "coffee-script": "^1.12.4",
    "css-loader": "^0.28.0",
    "extract-loader": "^0.1.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "html-loader": "^0.4.5",
    "node-sass": "^4.5.2",
    "pug": "^2.0.0-beta11",
    "pug-html-loader": "^1.1.4",
    "pug-loader": "^2.3.0",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.16.1",
    "webpack": "^2.3.3",
    "yaml-loader": "^0.4.0"
  }
}
